{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Http, Purge",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/cmd/nnf_ec.go",
            "args": ["--http", "--port=8080", "--purge"],
            "dlvLoadConfig": {
                "followPointers": true,
                "maxVariableRecurse": 1,
                "maxStringLen": 120,
                "maxArrayValues": 64,
                "maxStructFields": -1
            }
        },
        {
            "name": "Http, Cli, Purge",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/cmd/nnf_ec.go",
            "args": ["--http", "--port=8080", "--cli", "--purge"],
        },
        {
            "name": "Http, Mock",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/cmd/nnf_ec.go",
            "args": ["--mock", "--http", "--port=8080"],
        },
    ]
}